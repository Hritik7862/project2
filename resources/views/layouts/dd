<script>
    function showMemberDropdown() {
        $('#memberDropdown').css('display', 'block');
    }

    $('#selectMembers').on('change', function () {
        var selectedOption = $('#selectMembers option:selected');
        var selectedUserId = selectedOption.val();
        var selectedUserName = selectedOption.text();

        $('#tm').val($('#tm').val() + selectedUserId + ',');
        $('#tm2').val($('#tm2').val() + ', ' + selectedUserName);

        $('#memberDropdown').css('display', 'none');
    });

    // Show dropdown when clicking on the input field
    $('#tm2').on('click', function () {
        showMemberDropdown();
    });
</script>


