<div class="d-flex">
    <form action="{{ route('task.complete', $task->id) }}" method="POST">
        @csrf
        <input type="checkbox" class="task-complete-checkbox" id="task-complete-{{ $task->id }}" data-task-id="{{ $task->id }}" {{ $task->is_completed ? 'checked' : '' }}>
        <button type="button" id="complete-button-{{ $task->id }}" class="btn btn-primary" style="display: none;">Complete</button>
