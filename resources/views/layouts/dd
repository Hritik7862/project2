<script>



function openNav() {
  document.getElementById("mySidenav").style.width = "250px";
}

function closeNav() {
  document.getElementById("mySidenav").style.width = "0";
}
$(document).ready(function () {
    var storedCount = localStorage.getItem('notificationCount');

    var count = storedCount ? parseInt(storedCount) : 0;

    $('#notificationCount').text(count);

    $('#notificationsDropdown').on('click', function (evt) {
        $.ajax({
            url: 'remarks',
            method: 'get',
            success: function (data) {
                var newCount = data.length;
                var html = '';

                if (newCount > 0) {
                    for (let i = 0; i < newCount; i++) {
                        html += `
                            <a class="dropdown-item" href="#">
                                <h4>${i + 1} ${data[i].assigned_to.name}</h4>
                                ${data[i].remarks}
                                ${data[i].task_name}
                            </a>`;
                    }
                } else {
                    html = '<p class="dropdown-item">No notifications available</p>';
                }

                $('#notificationList').html(html);
                $('#notificationCount').text(newCount);

                localStorage.setItem('notificationCount', newCount.toString());
            }
        });
    });
});

</script>

