<?php

namespace App\Http\Controllers;

use App\Models\Task;
use Illuminate\Http\Request;
use Illuminate\Support\Facades\Auth;
use Illuminate\Support\Facades\View;


class AdminController extends Controller
{

    public function index()
{
    $adminId = Auth::user()->id;

    $remarks = Task::where(function ($query) use ($adminId) {
            $query->where('assigned_by', $adminId)
                  ->orWhere(function ($query) use ($adminId) {
                      $query->where('assigned_to', $adminId)
                            ->where('assigned_by', '<>', $adminId);
                  });
        })
        ->whereNotNull('remarks')
        ->get();

    $html = View::make('admin.remarks', compact('remarks'))->render();

    return response()->json(['html' => $html]);
}
}


<div class="container">
  
    <div class="modal fade" id="myModal" role="dialog">
      <div class="modal-dialog">
      
        <!-- Modal content-->
        <div class="modal-content">
          <div class="modal-header text-white" style="background-color:rgb(0, 0, 139);">
          <h4 class="modal-title">Notifcion</h4>
          </div>
          <div class="modal-body" id="datashow" >
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-dark" data-dismiss="modal">Close</button>
          </div>
        </div>
        
      </div>
    </div>

    $('#remarks').on('click', function(evt) {
        $.ajax({
            url: 'remarks',
            method: 'get',
            success: function(data) {
                var count = data.length;
                var html = '';
    
                if (count > 0) {
                    for (let i = 0; i < count; i++) {
                        // Use <div> elements to separate the data
                        html += `
                            <div>
                            <li>
                               <h4> ${i + 1} ${data[i].assigned_to.name}</h4>
                         
                            
                                ${data[i].remarks}
                           
                                ${data[i].task_name}
                           </li>
                            <br>`;
                    }
                    $('#datashow').html(html); 
                } else {
                    $('#datashow').html('<p>No data available</p>');
                }
    
                $('#notificationCount').text(count);
            }
        });
    });
    


    $(document).ready(function () {
            function fetchNotifications() {
                $.ajax({
                    url: 'your_notification_endpoint', // Replace with your actual endpoint
                    method: 'GET',
                    success: function (data) {
                        var notificationsContainer = $('#notificationsContainer');
                        notificationsContainer.empty();

                        if (data.length > 0) {
                            for (var i = 0; i < data.length; i++) {
                                var notification = data[i];
                                var notificationHtml = '<div class="notification">' +
                                    '<h4>' + notification.title + '</h4>' +
                                    '<p>' + notification.message + '</p>' +
                                    '</div>';
                                notificationsContainer.append(notificationHtml);
                            }
                        } else {
                            notificationsContainer.html('<p>No notifications available.</p>');
                        }
                    },
                    error: function () {
                        console.error('Error fetching notifications');
                    }
                });
            }

            // Call fetchNotifications on page load
            fetchNotifications();

            setInterval(fetchNotifications, 300000); 
        });


