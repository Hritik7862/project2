<?php 
function countOnesInBinary($decimalNumber) {
    $binaryRepresentation = decbin($decimalNumber); 
    $count = substr_count($binaryRepresentation, '1'); 
    return $count;
}

$number1 = 7;
$number2 = 8;

$count1 = countOnesInBinary($number1);
$count2 = countOnesInBinary($number2);

echo "$number1 ($count1)" . PHP_EOL;
echo "$number2 ($count2)" . PHP_EOL;

?>
$role=Role::all();
return view('register',compact('role'));



    protected function validator(array $data)
    {
        return Validator::make($data, [
            'name' => ['required', 'string', 'max:255'],
            'email' => ['required', 'string', 'email', 'max:255', 'unique:users', 'regex:/(.+)@(.+)\.(.+)/i'],
            'password' => ['required', 'string', 'min:8', 'confirmed'],
            'mobile' => ['required', 'string', 'size:10'],
        ]);
    }

    protected function create(array $data)
    {
        $user = User::create([
            'name' => $data['name'],
            'user_name' => $data['user_name'],
            'email' => $data['email'],
            'password' => Hash::make($data['password']),
            'mobile' => $data['mobile'],
            'is_active' => $data['is_active'],
            'admin' => 0, // Default value
        ]);
    
        $role = Role::find($data['role']);
        if ($role && $role->name === 'superadmin') {
            $user->admin = 1; 
            $user->save();
        }
    
        $user->assignRole($data['role']); 
    
        return $user->with('success', 'User registered successfully.');
    }
